{
  "name": "landing-page-cms",
  "services": [
    {
      "type": "app",
      "data": {
        "projectName": "landing-page-cms",
        "serviceName": "app",
        "source": {
          "type": "github",
          "owner": "seu-usuario",
          "repo": "landing-page-cms",
          "branch": "main"
        },
        "build": {
          "type": "dockerfile",
          "dockerfile": "Dockerfile"
        },
        "deploy": {
          "replicas": 1,
          "command": null,
          "zeroDowntime": true
        },
        "env": [
          {
            "key": "NODE_ENV",
            "value": "production"
          },
          {
            "key": "PORT",
            "value": "3000"
          }
        ],
        "mounts": [],
        "ports": [
          {
            "published": 3000,
            "target": 3000,
            "protocol": "tcp"
          }
        ],
        "domains": [
          {
            "host": "yourdomain.com",
            "port": 3000,
            "https": true
          }
        ]
      }
    },
    {
      "type": "app",
      "data": {
        "projectName": "landing-page-cms",
        "serviceName": "nginx",
        "source": {
          "type": "image",
          "image": "nginx:alpine"
        },
        "deploy": {
          "replicas": 1,
          "command": null,
          "zeroDowntime": true
        },
        "env": [],
        "mounts": [
          {
            "type": "file",
            "content": "# Nginx configuration will be mounted here",
            "mountPath": "/etc/nginx/nginx.conf"
          }
        ],
        "ports": [
          {
            "published": 80,
            "target": 80,
            "protocol": "tcp"
          },
          {
            "published": 443,
            "target": 443,
            "protocol": "tcp"
          }
        ],
        "domains": [
          {
            "host": "yourdomain.com",
            "port": 80,
            "https": true
          }
        ]
      }
    }
  ]
}
